{
  "expo": {
    "name": "Return It - Driver",
    "slug": "returnit-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "../assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "../assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#B8956A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.returnit.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "ReturnIt needs location access to optimize pickup and delivery routes for drivers.",
        "NSLocationAlwaysUsageDescription": "ReturnIt uses location to provide real-time tracking for customers and route optimization for drivers.",
        "NSCameraUsageDescription": "ReturnIt needs camera access to capture package photos for delivery confirmation.",
        "NSPhotoLibraryUsageDescription": "ReturnIt needs photo library access to attach package photos and receipts."
      }
    },
    "android": {
      "package": "com.returnit.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "../assets/adaptive-icon.png",
        "backgroundColor": "#B8956A"
      },
      "compileSdkVersion": 35,
      "targetSdkVersion": 35,
      "buildToolsVersion": "35.0.0",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "POST_NOTIFICATIONS",
        "VIBRATE"
      ],
      "usesCleartextTraffic": false,
      "allowBackup": false
    },
    "notification": {
      "color": "#B8956A",
      "androidMode": "default",
      "androidCollapsedTitle": "New delivery available"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "kotlinVersion": "1.9.22"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "ReturnIt needs camera access to capture package photos for delivery confirmation."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "ReturnIt uses location to provide real-time tracking and route optimization."
        }
      ]
    ],
    "cli": {
      "appVersionSource": "remote"
    },
    "extra": {
      "eas": {
        "projectId": "9225a988-ab9f-4a64-824a-06b91366c3ee"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/9225a988-ab9f-4a64-824a-06b91366c3ee"
    },
    "privacy": "public"
  }
}
