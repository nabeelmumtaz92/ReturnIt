{
  "expo": {
    "name": "ReturnIt - Return Delivery",
    "slug": "returnit-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://via.placeholder.com/1024x1024/D2691E/FFFFFF?text=R",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "https://via.placeholder.com/1284x2778/D2691E/FFFFFF?text=ReturnIt",
      "resizeMode": "contain",
      "backgroundColor": "#D2691E"
    },
    "assetBundlePatterns": [],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.returnit.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "ReturnIt needs location access to optimize pickup and delivery routes for drivers.",
        "NSLocationAlwaysUsageDescription": "ReturnIt uses location to provide real-time tracking for customers and route optimization for drivers.",
        "NSCameraUsageDescription": "ReturnIt needs camera access to capture package photos for delivery confirmation.",
        "NSPhotoLibraryUsageDescription": "ReturnIt needs photo library access to attach package photos and receipts."
      }
    },
    "android": {
      "package": "com.returnit.app",
      "compileSdkVersion": 35,
      "targetSdkVersion": 35,
      "buildToolsVersion": "35.0.0",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "usesCleartextTraffic": false,
      "allowBackup": false
    },
    "cli": {
      "appVersionSource": "remote"
    },
    "extra": {
      "eas": {
        "projectId": "9225a988-ab9f-4a64-824a-06b91366c3ee"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
